{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "BinderBase/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "BinderBase/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/BinderBase/$1"
    }
  ]
}